{
  "kind": "collectionType",
  "collectionName": "faq_groups",
  "info": {
    "singularName": "faq-group",
    "pluralName": "faq-groups",
    "displayName": "Page.FAQ Group",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "presentation_mode": {
      "type": "enumeration",
      "enum": [
        "B2C",
        "B2B",
        "Both"
      ],
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "pages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::faq-page.faq-page",
      "mappedBy": "group"
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "title",
      "required": true
    }
  }
}
